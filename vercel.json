{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/" }],
  "functions": {
    "api/updateProject": {
      "memory": 1024,
      "maxRequestBodySize": "1gb"
    }
  }
}
